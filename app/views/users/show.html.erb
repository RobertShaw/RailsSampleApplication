<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
	<%= gravatar_for @user %>
	<%= @user.name %>, <%= @user.email %>, <%= @user.created_at.to_s + ', ' + @user.updated_at.to_s + ', ' + Time.now.to_s %>
      </h1>


    </section>
  </aside>
</div>
